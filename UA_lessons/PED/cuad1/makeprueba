OPTS=-Wall -g -std=c++11
OBJS=prueba.o tporo.o tvectorporo.o tlistaporo.o
CC=g++

prueba: $(OBJS)
	$(CC) $(OPTS) $(OBJS) -o prueba

prueba.o: prueba.cpp tporo.h
	$(CC) $(OPTS) -c prueba.cpp

tporo.o: tporo.cpp tporo.h 
	$(CC) $(OPTS) -c tporo.cpp

tvectorporo.o: tvectorporo.cpp tvectorporo.h tporo.h
	$(CC) $(OPTS) -c tvectorporo.cpp

tlistaporo.o: tlistaporo.cpp tlistaporo.h tvectorporo.h tporo.h
	$(CC) $(OPTS) -c tlistaporo.cpp

clean:
	rm $(OBJS)
